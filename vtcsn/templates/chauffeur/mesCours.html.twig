{% extends 'base.html.twig' %}


{% block title %}Mes Command
{% endblock %}

{% block body %}


	<div class="container-fluid profil">

		<div class="row justify-content-center">
			<fieldset class=" col-ms-12 col-md-5 col-lg-3  register " class=" maClasse" style=" overflow-y:scroll">
				<h5>Commandes de{{app. user.surName }}</h5>

				{% for ride in  rides %}
					<div class=" cardh-100  rating">
						<h6 class="card-title">Trajet faie le :{{ride.createdAt |date('d-m-y')}}
						</h6>
						<p class="card-text">De :
							{{ride.rendezVous}}</p>
						<p class="card-text">Ver :
							{{ride.destination}}</p>

					</div>
				{% endfor %}
			</fieldset>

		</div>
	{% endblock %}
